<?php $this->extend('block_unsearchable'); ?>

<?php $this->block('content'); ?>

  <?php if (empty($this->products)): ?>
    <p class="empty"><?php echo $this->empty; ?></p>
  <?php else: ?>
    <?php echo implode('', $this->products); ?>
    <?php echo $this->pagination; ?>
  <?php endif; ?>

<?php $this->endblock(); ?>
